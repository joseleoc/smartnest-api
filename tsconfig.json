{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "lib": ["ESNext"],
    "types": ["@cloudflare/workers-types"],
    // "jsx": "react-jsx",
    // "jsxImportSource": "hono/jsx",
    "baseUrl": "./src",
    "paths": {
      "@controllers/*": ["controllers/*"],
      "@middlewares/*": ["middlewares/*"],
      "@models/*": ["models/*"],
      "@routes/*": ["routes/*"],
      "@schemas/*": ["schemas/*"],
      "@db/*": ["db/*"]
    }
  }
}
